        "ioredis": "^5.3.2",
        "winston": "^3.11.0",
        "express-async-errors": "^3.1.1",
        "joi": "^17.11.0"
    },
    "devDependencies": {
        "nodemon": "^3.0.2",
        "eslint": "^8.56.0",
        "prettier": "^3.1.1",
