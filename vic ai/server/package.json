{
    "name": "vic-ai-server",
    "version": "2.0.0-beta",
    "description": "Enhanced backend server for Vic-AI-Beta with advanced AI processing, security, and monitoring",
    "main": "index.js",
    "type": "module",
    "exports": {
        ".": "./index.js",
        "./routes": "./routes/index.js",
        "./services": "./services/index.js",
        "./middleware": "./middleware/index.js",
        "./config": "./config/index.js"
    },
    "scripts": {
        "start": "node index.js",
        "dev": "node --watch index.js",
        "dev:debug": "node --inspect --watch index.js",
        "test": "node --test",
        "lint": "eslint . --ext .js",
        "format": "prettier --write ."
    },
    "keywords": [
        "ai",
        "server",
        "backend",
        "express",
        "vic-ai",
        "api",
        "security",
        "monitoring",
        "rate-limiting"
